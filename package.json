    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.9",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coffeescript": "^2.5.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^6.2.3",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "author": "Pagan Gazzard <page@balena.io>",
