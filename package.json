{
  "name": "typed-error",
  "version": "1.1.0",
  "description": "A class designed to enable easily extending the built-in javascript Error, allowing typed errors.",
  "main": "src/typed-error.js",
  "types": "src/typed-error.ts",
  "scripts": {
    "prepublish": "require-npm4-to-publish",
    "prepublishOnly": "npm test",
    "pretest": "npm run build",
    "build": "npm run prettify && tsc",
    "prettify": "prettier --use-tabs --single-quote --write src/*.ts",
    "test": "mocha --compilers ts:ts-node/register && mocha --compilers coffee:coffee-script/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resin-io/typed-error.git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.14",
    "bluebird": "^3.5.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coffee-script": "~1.12.0",
    "mocha": "^3.4.2",
    "prettier": "^1.5.3",
    "require-npm4-to-publish": "^1.0.0",
    "ts-node": "^3.2.1",
    "typescript": "2.4.2"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/resin-io/typed-error/issues"
  },
  "homepage": "https://github.com/resin-io/typed-error",
  "dependencies": {
    "tslib": "^1.7.1"
  }
}