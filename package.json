    "build": "tsc --pretty --project ./tsconfig.dist.json",
    "test": "mocha",
    "pretest": "npm run build",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resin-io/typed-error.git"
  },
