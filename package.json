    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.36",
    "bluebird": "^3.5.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coffeescript": "^2.3.2",
    "mocha": "^5.2.0",
    "prettier": "^1.14.3",
    "resin-lint": "^2.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "author": "Pagan Gazzard <page@resin.io>",
